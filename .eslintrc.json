{
    "extends": ["airbnb/base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "ignorePatterns": [
        "*.ejs"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "node/no-unpublished-require": "off"
    }
}
